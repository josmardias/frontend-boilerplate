  "description": "A boilerplate to quick start frontend projects. Powered by React and Webpack. No SSR by design.",
  "license": "MIT",
  "author": "Josmar Dias",
  "homepage": "https://github.com/josmardias/frontend-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/josmardias/frontend-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josmardias/frontend-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "redux",
    "starter"
  ],
  "main": "index.js",
  "scripts": {
    "fixpack": "fixpack",
    "lint": "eslint .",
